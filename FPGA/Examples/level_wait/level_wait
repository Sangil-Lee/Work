#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x270e3a0 .scope module, "level_wait" "level_wait" 2 1;
 .timescale 0 0;
v0x270e4f0_0 .var "data", 7 0;
v0x2741810_0 .var "data_bus", 7 0;
v0x27418b0_0 .var "data_ready", 0 0;
v0x2741950_0 .var "mem_read", 0 0;
E_0x270e490 .event edge, v0x27418b0_0, v0x2741810_0, v0x2741950_0;
E_0x270e4c0 .event edge, v0x27418b0_0;
    .scope S_0x270e3a0;
T_0 ;
    %wait E_0x270e490;
    %set/v v0x270e4f0_0, 0, 8;
T_0.0 ;
    %load/v 8, v0x2741950_0, 1;
    %jmp/0xz T_0.1, 8;
T_0.2 ;
    %load/v 8, v0x27418b0_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %cmpi/u 4, 1, 1;
    %inv 6, 1;
    %jmp/0xz T_0.3, 6;
    %wait E_0x270e4c0;
    %jmp T_0.2;
T_0.3 ;
    %delay 1, 0;
    %load/v 8, v0x2741810_0, 8;
    %set/v v0x270e4f0_0, 8, 8;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x270e3a0;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "./level_wait.vcd";
    %vpi_call 2 16 "$dumpvars", 1'sb0, S_0x270e3a0;
    %vpi_call 2 18 "$monitor", "%d Read = %b, Ready = %b, Data=%b", $time, v0x2741950_0, v0x27418b0_0, v0x270e4f0_0;
    %set/v v0x2741810_0, 0, 8;
    %set/v v0x2741950_0, 0, 1;
    %set/v v0x27418b0_0, 0, 1;
    %delay 10, 0;
    %movi 8, 222, 8;
    %set/v v0x2741810_0, 8, 8;
    %delay 10, 0;
    %set/v v0x2741950_0, 1, 1;
    %delay 20, 0;
    %set/v v0x27418b0_0, 1, 1;
    %delay 1, 0;
    %set/v v0x27418b0_0, 0, 1;
    %delay 10, 0;
    %movi 8, 173, 8;
    %set/v v0x2741810_0, 8, 8;
    %delay 20, 0;
    %set/v v0x27418b0_0, 1, 1;
    %delay 1, 0;
    %set/v v0x27418b0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 33 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "level_wait.v";
