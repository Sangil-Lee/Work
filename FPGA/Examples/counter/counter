#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1fcdb40 .scope module, "counter_tb" "counter_tb" 2 1;
 .timescale 0 0;
v0x2001bc0_0 .var "clk", 0 0;
v0x2001c90_0 .var "reset", 0 0;
v0x2001d40_0 .net "value", 7 0, v0x2001a80_0; 1 drivers
S_0x1ff03f0 .scope module, "c1" "counter" 2 17, 3 1, S_0x1fcdb40;
 .timescale 0 0;
P_0x1fef7b8 .param/l "WIDTH" 3 3, +C4<01000>;
v0x1ff0510_0 .net "clk", 0 0, v0x2001bc0_0; 1 drivers
v0x2001a80_0 .var "out", 7 0;
v0x2001b20_0 .net "reset", 0 0, v0x2001c90_0; 1 drivers
E_0x1fcd500 .event edge, v0x2001b20_0;
E_0x1ff04e0 .event posedge, v0x1ff0510_0;
    .scope S_0x1ff03f0;
T_0 ;
    %wait E_0x1ff04e0;
    %load/v 8, v0x2001a80_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v0x2001a80_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1ff03f0;
T_1 ;
    %wait E_0x1fcd500;
    %load/v 8, v0x2001b20_0, 1;
    %jmp/0xz  T_1.0, 8;
    %cassign/v v0x2001a80_0, 0, 8;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x2001a80_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x1fcdb40;
T_2 ;
    %set/v v0x2001c90_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x1fcdb40;
T_3 ;
    %delay 17, 0;
    %set/v v0x2001c90_0, 1, 1;
    %delay 11, 0;
    %set/v v0x2001c90_0, 0, 1;
    %delay 29, 0;
    %set/v v0x2001c90_0, 1, 1;
    %delay 11, 0;
    %set/v v0x2001c90_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 9 "$stop";
    %end;
    .thread T_3;
    .scope S_0x1fcdb40;
T_4 ;
    %set/v v0x2001bc0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x1fcdb40;
T_5 ;
    %delay 5, 0;
    %load/v 8, v0x2001bc0_0, 1;
    %inv 8, 1;
    %set/v v0x2001bc0_0, 8, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x1fcdb40;
T_6 ;
    %vpi_call 2 21 "$dumpfile", "./counter_tb.vcd";
    %vpi_call 2 22 "$dumpvars", 1'sb0, S_0x1fcdb40;
    %vpi_call 2 24 "$monitor", "At Time %t, value = %h (%0d)", $time, v0x2001d40_0, v0x2001d40_0;
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
