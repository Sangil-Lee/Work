#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1e26aa0 .scope module, "edge_wait" "edge_wait" 2 1;
 .timescale 0 0;
v0x1e26bc0_0 .var "clk", 0 0;
v0x1e59290_0 .var "enable", 0 0;
v0x1e59330_0 .var "trigger", 0 0;
E_0x1e137e0 .event posedge, v0x1e59290_0;
E_0x1e26b90 .event posedge, v0x1e26bc0_0;
    .scope S_0x1e26aa0;
T_0 ;
    %wait E_0x1e137e0;
    %set/v v0x1e59330_0, 0, 1;
    %movi 8, 5, 4;
T_0.0 %cmp/s 0, 8, 4;
    %jmp/0xz T_0.1, 5;
    %add 8, 1, 4;
    %wait E_0x1e26b90;
    %jmp T_0.0;
T_0.1 ;
    %set/v v0x1e59330_0, 1, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1e26aa0;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "./edge_wait.vcd";
    %vpi_call 2 16 "$dumpvars", 1'sb0, S_0x1e26aa0;
    %vpi_call 2 17 "$monitor", "Time:%d, Clk:%b, Enable:%b, Trigger:%b", $time, v0x1e26bc0_0, v0x1e59290_0, v0x1e59330_0;
    %set/v v0x1e26bc0_0, 0, 1;
    %set/v v0x1e59290_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1e59290_0, 1, 1;
    %delay 1, 0;
    %set/v v0x1e59290_0, 0, 1;
    %delay 10, 0;
    %set/v v0x1e59290_0, 1, 1;
    %delay 1, 0;
    %set/v v0x1e59290_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 24 "$finish";
    %end;
    .thread T_1;
    .scope S_0x1e26aa0;
T_2 ;
    %delay 1, 0;
    %load/v 8, v0x1e26bc0_0, 1;
    %inv 8, 1;
    %set/v v0x1e26bc0_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "edge_wait.v";
