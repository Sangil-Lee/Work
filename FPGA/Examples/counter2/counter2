#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x26dbfc0 .scope module, "counter2_tb" "counter2_tb" 2 2;
 .timescale 0 0;
v0x26efd70_0 .var "clk", 0 0;
v0x26efe40_0 .net "counter_out", 3 0, v0x26efb80_0; 1 drivers
v0x26efef0_0 .var "enable", 0 0;
v0x26effa0_0 .var "reset", 0 0;
S_0x26dc0b0 .scope module, "count" "counter2" 2 27, 3 1, S_0x26dbfc0;
 .timescale 0 0;
v0x26dd9f0_0 .net "clk", 0 0, v0x26efd70_0; 1 drivers
v0x26efb80_0 .var "counter_out", 3 0;
v0x26efc20_0 .net "enable", 0 0, v0x26efef0_0; 1 drivers
v0x26efcc0_0 .net "reset", 0 0, v0x26effa0_0; 1 drivers
E_0x26bae90 .event posedge, v0x26dd9f0_0;
S_0x26dd900 .scope begin, "COUNTER" "COUNTER" 3 10, 3 10, S_0x26dc0b0;
 .timescale 0 0;
    .scope S_0x26dc0b0;
T_0 ;
    %wait E_0x26bae90;
    %fork t_1, S_0x26dd900;
    %jmp t_0;
    .scope S_0x26dd900;
t_1 ;
    %load/v 8, v0x26efcc0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0x26efb80_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x26efc20_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x26efb80_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v0x26efb80_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_0x26dc0b0;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x26dbfc0;
T_1 ;
    %vpi_call 2 8 "$display", "time\011 clk, reset, enable, counter";
    %vpi_call 2 9 "$monitor", "%g\011 %b, %b, %b, %b", $time, v0x26efd70_0, v0x26effa0_0, v0x26efef0_0, v0x26efe40_0;
    %vpi_call 2 10 "$dumpfile", "./counter2_tb.vcd";
    %vpi_call 2 11 "$dumpvars", 1'sb0, S_0x26dbfc0;
    %set/v v0x26efd70_0, 1, 1;
    %set/v v0x26effa0_0, 0, 1;
    %set/v v0x26efef0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x26effa0_0, 1, 1;
    %delay 10, 0;
    %set/v v0x26effa0_0, 0, 1;
    %delay 5, 0;
    %set/v v0x26efef0_0, 1, 1;
    %delay 125, 0;
    %set/v v0x26efef0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 20 "$finish";
    %end;
    .thread T_1;
    .scope S_0x26dbfc0;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x26efd70_0, 1;
    %inv 8, 1;
    %set/v v0x26efd70_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter2_tb.v";
    "./counter2.v";
