# Should not fit in a CA transfer!
record(waveform,"lca:wavA") {
	field("NELM", "20000")
	field("FTVL", "FLOAT")
	field("EGU",  "testU")
	field("PREC", "4")
	field("HOPR", "111")
	field("LOPR", "-222")
	field("PINI", "YES")
}

record(calc,    "lca:count") {
	field("INPA","lca:count")
	field("CALC","A+1")
	field("SCAN","1 second")
}

