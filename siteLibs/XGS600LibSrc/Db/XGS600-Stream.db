record(stringin, "$(P)XGS600:SW_VER")
{
    field(DESC, "XGS-600 Software ver.")
    field(DTYP, "stream")
    field(INP, "@XGS600.proto sw-ver-si(00) $(PORT)")
    field(PINI, "YES")
}
record(ai, "$(P)XGS600:PRES1")
{
    field(DESC, "Pressure ch1")
    field(DTYP, "stream")
    field(INP, "@XGS600.proto press1_si(00) $(PORT)")
    field(PINI, "NO")
    field(SCAN, "1 second")
    field(EGU, "Torr")
}
