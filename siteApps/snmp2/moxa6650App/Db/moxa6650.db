
# Ethernet Connection Status
record(snmp, "${SYS}-${SUBSYS}:${DEV}:Port1") {
  field(DESC, "Moxa 6650 NPort")
  field(DTYP, "SNMP Read")
  field(SCAN, "5 second")
  field(VERS, "${V2C}")
  field(HOST, "${HOST}")
  field(COMM, "${COM}")
  field(OIDS, "IF-MIB::ifOperStatus.1")
}

