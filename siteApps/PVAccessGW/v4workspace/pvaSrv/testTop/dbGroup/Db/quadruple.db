record(waveform, "$(name):POLYNOMIAL")
{
        field(NELM,"5")
        field(NORD,"5")
        field(FTVL,"DOUBLE")
}

record(ai, "$(name):Current")
{
        field(PREC, "1")
        field(EGU, "amps")
        field(HOPR, "10")
        field(LOPR, "0")
        field(HIHI, "8")
        field(HIGH, "6")
        field(LOW, "4")
        field(LOLO, "2")
        field(HHSV, "MAJOR")
        field(HSV, "MINOR")
        field(LSV, "MINOR")
        field(LLSV, "MAJOR")
}

