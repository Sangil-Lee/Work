# ao record template for register outputs
record(ao, "$(SYS)$(SUBSYS)$(DEV)$(SUBDEV)$(SIGNAL)") {
    field(DTYP,	"asynFloat64")
    field(OUT,	"@asyn($(PORT) $(OFFSET))$(DATA_TYPE)")
	field(OMSL, "closed_loop")
    field(HOPR,	"$(HOPR)")
    field(LOPR,	"$(LOPR)")
    field(PREC,	"$(PREC)")
    field(EGU,	"$(EGU)")
    field(VAL,	"0")
}

