		rangeStyle="auto-scale"
		maxRange=0.000000
	}
	y1_axis {
		rangeStyle="auto-scale"
		minRange=-6.000000
		maxRange=6.000000
	}
	y2_axis {
		maxRange=0.000000
	}
	trigger="$(scope)scaledTimeAxisWF"
	erase="$(scope)erasePlotBO"
}
