"cartesian plot" {
	object {
		x=59
		y=51
		width=200
		height=150
	}
	plotcom {
		title="Channel #1"
		xlabel="Microseconds"
		ylabel="Volts"
		clr=15
		bclr=14
	}
	style="line"
	erase_oldest="plot last n pts"
	count="0"
	trace[0] {
		xdata="$(scope)scaledTimeAxisWF"
		ydata="$(scope)chan1ScaledWaveWF"
		data_clr=30
	}
