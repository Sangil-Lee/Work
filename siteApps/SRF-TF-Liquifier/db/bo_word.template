# bo record template for register outputs
record(bo, "$(SYS)$(SUBSYS):$(DEV)$(SUBDEV):$(FIELD)") {
    field(DTYP,"asynUInt32Digital")
    #field(DTYP,"asynOctet")
    field(OUT,"@asynMask($(PORT) $(OFFSET) $(MASK))BCD_UNSIGNED")
    field(ZNAM,"$(ZNAM)")
    field(ONAM,"$(ONAM)")
    field(PINI,"NO")
}
