# bo record template for register outputs
record(bo,"$(P)$(R)") {
    field(DTYP,"asynUInt32Digital")
    field(OUT,"@asynMask($(PORT) $(OFFSET) 0x1)")
    field(ZNAM,"$(ZNAM)")
    field(ONAM,"$(ONAM)")
    field(SCAN,"$(SCAN)")
    field(FLNK,"$(FLNK)")
    field(OMSL,"$(OMSL)")
    field(DOL,"$(DOL)")
}
