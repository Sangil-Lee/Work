program CDOperationSeq
option +r;

%% #include <math.h>

int proc;
assign proc "SCL31-CDL01:FwrdLink.PROC";

ss steadystate
{
	state init
	{
		when(TRUE) 
		{
			//initialization
			proc = 1;
			pvPut(proc, SYNC);
		} state processing
	}

	state processing
	{
		when(delay(1.0))
		{
			proc = 1;
			pvPut(proc, SYNC);
			printf("Cool Down Processing...\n");
		}state processing
	}

}

